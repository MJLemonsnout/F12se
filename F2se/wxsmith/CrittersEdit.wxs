					</object>
					<object class="sizeritem">
						<object class="wxListBox" name="ID_LISTBOX2" subclass="MyListBox" variable="lbBaseStats" member="yes">
							<default>-1</default>
							<handler function="OnlbBaseStatsSelect" entry="EVT_LISTBOX" />
						</object>
						<flag>wxALL|wxEXPAND</flag>
						<border>5</border>
						<option>1</option>
